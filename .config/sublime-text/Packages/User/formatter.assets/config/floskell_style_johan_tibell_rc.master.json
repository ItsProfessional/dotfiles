{
    "extensions": [],
    "fixities": [],
    "formatting": {
        "align": {
            "case": false,
            "class": false,
            "import-module": false,
            "import-spec": false,
            "let-binds": false,
            "limits": [
                10,
                25
            ],
            "matches": false,
            "record-fields": false,
            "where": false
        },
        "group": {
            "default": {
                "force-linebreak": false,
                "linebreaks": "none",
                "spaces": "none"
            },
            "{": {
                "force-linebreak": false,
                "linebreaks": "after",
                "spaces": "both"
            },
            "{ in pattern": {
                "force-linebreak": false,
                "linebreaks": "none",
                "spaces": "none"
            }
        },
        "indent": {
            "app": "indent-by 4",
            "case": "indent-by 4",
            "class": "indent-by 4",
            "deriving": 4,
            "do": "indent-by 4",
            "export-spec-list": "indent-by 2",
            "if": "indent-by 4",
            "import-spec-list": "align-or-indent-by 7",
            "let": "align",
            "let-binds": "align",
            "let-in": "align",
            "multi-if": "indent-by 2",
            "onside": 4,
            "typesig": "align",
            "where": 2,
            "where-binds": "indent-by 2"
        },
        "layout": {
            "app": "try-oneline",
            "con-decls": "vertical",
            "declaration": "try-oneline",
            "export-spec-list": "try-oneline",
            "if": "vertical",
            "import-spec-list": "flex",
            "infix-app": "try-oneline",
            "let": "vertical",
            "list-comp": "flex",
            "record": "vertical",
            "type": "try-oneline"
        },
        "op": {
            ",": {
                "force-linebreak": false,
                "linebreaks": "before",
                "spaces": "after"
            },
            ", in other": {
                "force-linebreak": false,
                "linebreaks": "before",
                "spaces": "none"
            },
            ", in pattern": {
                "force-linebreak": false,
                "linebreaks": "before",
                "spaces": "none"
            },
            ". in type": {
                "force-linebreak": false,
                "linebreaks": "after",
                "spaces": "after"
            },
            ": in pattern": {
                "force-linebreak": false,
                "linebreaks": "before",
                "spaces": "none"
            },
            "=": {
                "force-linebreak": false,
                "linebreaks": "after",
                "spaces": "both"
            },
            "default": {
                "force-linebreak": false,
                "linebreaks": "before",
                "spaces": "both"
            },
            "record": {
                "force-linebreak": true,
                "linebreaks": "after",
                "spaces": "after"
            },
            "record in pattern": {
                "force-linebreak": false,
                "linebreaks": "after",
                "spaces": "after"
            }
        },
        "options": {
            "align-let-binds-and-in-expr": false,
            "align-sum-type-decl": true,
            "decl-no-blank-lines": [],
            "flexible-oneline": true,
            "preserve-vertical-space": false,
            "sort-import-lists": false,
            "sort-imports": false,
            "sort-pragmas": false,
            "split-language-pragmas": false
        },
        "penalty": {
            "indent": 1,
            "linebreak": 100,
            "max-line-length": 80,
            "overfull": 10,
            "overfull-once": 200
        }
    },
    "language": "Haskell2010",
    "style": "johan-tibell"
}